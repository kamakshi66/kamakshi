        "class Flower:\n",
        "      def __init__(self, petalName, petalNumber, petalPrice):\n",
        "        self.name = petalName\n",
        "        self.number = petalNumber\n",
        "        self.price = petalPrice\n",
        "      def setName(self, petalName):\n",
        "        self.name = petalName\n",
        "      def setPetals(self, petalNumber):\n",
